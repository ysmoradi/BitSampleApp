{
  "compileOnSave": true,
  "compilerOptions": {
    "allowUnreachableCode": true,
    "charset": "utf8",
    "diagnostics": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "inlineSources": true,
    "lib": [ "dom", "es2017" ],
    "noEmitOnError": false,
    "removeComments": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": [ "node_modules" ]
}